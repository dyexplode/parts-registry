{
  "name": "parts-registry",
  "version": "0.0.1",
  "description": "This is a repo for my home program, which organizes the saving of parts and a quick search in different boxes",
  "main": "server.ts",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -project tsconfig.json",
    "build:live": "nodemon --watch src --ext ts --exec ts-node src/server.ts"
  },
  "keywords": [
    "parts",
    "details",
    "components",
    "registry"
  ],
  "author": "Vladzimir Kakhanouski <admin@ttp.by>",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/node": "^18.11.17",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
